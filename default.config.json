{
    "port": 80,
    "hostname": null,
    "loggingLevel": 2,
    "ignoreErrors": true,
    "tls": false,
    "key": null,
    "cert": null,
    "additionalProxyServerOptions": {
        
    },
    "supportedVersions": [
        "HTTP/1.1"
    ],
    "defaultServerOptions": {
        "whitelist": "whitelist.json",
        "blacklist": null,
        "realIpHeader": "CF-Connecting-IP",
        "authorizationCookie": "HTTP-Reverse-Proxy-Authorization",
        "customAuthorizationHeader": "X-HTTP-Reverse-Proxy-Authorization",
        "authorizationType": "cookies",
        "modifiedHeaders": {
            "Host": "%{serverOptions.serverHostname}:%{serverOptions.serverPort}",
            "X-Forwarded-Proto": null,
            "CDN-Loop": null,
            "CF-Visitor": null,
            "CF-RAY": null,
            "Origin": null
        },
        "uriBypass": {
            "/robots.txt": {
                "statusCode": 200,
                "statusMessage": "OK",
                "headers": { "Content-Type": "text/plain" },
                "data": "User-agent: *\nDisallow: /"
            }
        }
    }
}
